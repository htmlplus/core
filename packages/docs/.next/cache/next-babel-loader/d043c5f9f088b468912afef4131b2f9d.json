AAC,KAAX;AAAiB,IAAA,GAAG,EAAC,SAArB;AAA+B,IAAA,GAAG,EAAED,OAApC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADF,CA1BF,EA6BE,MAAC,IAAD;AAAM,IAAA,EAAE,EAAC,qBAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,KAAK,EAAC,KAAX;AAAiB,IAAA,GAAG,EAAC,KAArB;AAA2B,IAAA,GAAG,EAAEG,GAAhC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADF,CA7BF,EAgCE,MAAC,IAAD;AAAM,IAAA,EAAE,EAAC,uBAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,KAAK,EAAC,KAAX;AAAiB,IAAA,GAAG,EAAC,OAArB;AAA6B,IAAA,GAAG,EAAED,KAAlC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADF,CAhCF,CADF;AAsCD,CAvCM;AAyCP,eAAeE,iBAAf","sourcesContent":["import React from 'react';\nimport * as constant from 'constant';\nimport { Link, Page, Text } from 'components';\n\nconst angular = '/assets/logo/angular.png';\nconst javascript = '/assets/logo/javascript.png';\nconst react = '/assets/logo/react.png';\nconst vue = '/assets/logo/vue.png';\n\nexport const FrameworkOverview: React.FC = () => {\n  return (\n    <Page layout=\"default\">\n      <Text>Overview</Text>\n      <Text>\n        در\n        {constant.PLATFORM_NAME}\n        هدف اصلی ما تمرکز بر تولید کامپوننت هایی است که در تمامی فریم ورک ها قابل استفاده باشد. برای\n        تحقق این امر ما از\n        <a href={constant.WEB_COMPONENT_REFERENCE}>ای پی ای های پلتفرم وب استاندارد</a>\n        شده استفاده کردیم که در تمام مرورگر های مدرن کار می کنند. فریم ورک هایی هستند که هنوز به طور\n        کامل از قابلیت های وب کامپوننت ها پشتیبانی نمی کنند شما می توانید از\n        <a href={constant.WEB_COMPONENT_COMPATIBILITY}>اینجا</a>\n        به لیست انها دسترسی پیدا کنید. اما نگران نباشید ما برای این چنین فریم ورک هایی پکیج های\n        جداگانه و مخصوص آنها تولید کرده ایم که مانند یک کتابخانه بومی عمل میکند. درست مثل اینکه از\n        ابتدا کامپوننت ها را با یکی از این فریم ورک ها تولید کرده اید. توجه داشته باشید که رشد فریم\n        ورک ها متوقف نمی شوند و در آینده فریم ورک های جدیدی پا به عرصه خواهند گذاشت پس رسالت ما رشد\n        همگام با کتابخانه های موجود و دنبال کردن کتابخانه های جدیدی که در آینده بوجود خواهند امد\n        برای سازگاری با انها هستیم برای شروع ما از کتابخانه ها و فریم ورک های زیر پشتیبانی میکنیم.\n        تمامی انها به طور کامل پشتیبانی نمیشوند اما در ایند نزدیک و با رشد\n        {constant.PLATFORM_NAME}\n        این امر تحقق پیدا خواهد کرد\n      </Text>\n      pic of react, angular, vue\n      <br />\n      <Link to=\"ROUTE:FRAMEWORK:JAVASCRIPT\">\n        <img width=\"100\" alt=\"javascript\" src={javascript} />\n      </Link>\n      <Link to=\"ROUTE:FRAMEWORK:ANGULAR\">\n        <img width=\"100\" alt=\"angular\" src={angular} />\n      </Link>\n      <Link to=\"ROUTE:FRAMEWORK:VUE\">\n        <img width=\"100\" alt=\"vue\" src={vue} />\n      </Link>\n      <Link to=\"ROUTE:FRAMEWORK:REACT\">\n        <img width=\"100\" alt=\"react\" src={react} />\n      </Link>\n    </Page>\n  );\n};\n\nexport default FrameworkOverview;\n"]},"metadata":{},"sourceType":"module"}